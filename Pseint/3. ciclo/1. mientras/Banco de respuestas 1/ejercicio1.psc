Algoritmo ejercicio1
	Definir num Como Entero
	Escribir "escriba un numero"
	Leer num
	contador=0
	disminuir=0
	Mientras num>contador Hacer
		contador=contador+1
		Escribir "los numeros son",contador
	Fin Mientras
	Mientras disminuir<contador Hacer
		disminuir=disminuir+1
		resta=contador-disminuir+1
		
		Escribir "los numeros son",resta

	FinMientras
	
FinAlgoritmo
